pipeline{
    
    agent any
        
        stages{
            
            stage("Build")
            {
                steps{
                    echo("Build")
                }
            }
            
            stage("Run UTs")
            {
                steps{
                    echo("Run UTs")
                }
            }
            
            stages("Deploy to dev")
            {
                step{
                    echo("dev deployment")
                }
            }
            
             stages("Deploy to qa")
            {
                step{
                    echo("qa deployment")
                }
            }
            
             stage("Run Automation Reg Test")
            {
                steps{
                    echo("Run Automation Reg Test")
                }
            }
            
             stage("Deploy to stage")
            {
                steps{
                    echo("stage deployment")
                }
            }
            
                 stage("Deploy to PROD")
            {
                steps{
                    echo("prod deployment")
                }
            }
            
        }
    
}